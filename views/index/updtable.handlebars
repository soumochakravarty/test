<h3>Update Table for a Restaurant</h3>
<div class="row">
  <form action="/api/table/update" method="post" class="col s12">
    <div class="row">
      <div class="input-field" >
        <input type="text" name="restaurant">
        <label for="restaurant">Restaurant</label>
      </div>
    </div>
    <div class="row">
      <div class="input-field" >
        <input type="text" name="table">
        <label for="table">No of Tables</label>
      </div>
    </div>
    </div>
    <input type="submit" value="Update" class="btn green accent-4 z-depth-3">
  </form>
</div>