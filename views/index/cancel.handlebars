<center><h3 class="gen-h1" >Cancel Calls</h3></center>
<div class="row">
  <form action="/api/cancel" method="post" class="col s12">
<div class="input-field col s12">
    <select name="call">
    <option value="" disabled selected>Select Open Call</option>
{{#each call_no}}
    <option value="{{this.tid}}">Table No: {{this.tid}} Wait Time: {{this.wait_time}} </option>
{{/each}}
    </select>
    <label class="blue-text text-darken-2"><b>Table No</b></label>
</div>
    <input type="submit" value="Cancel" class="waves-effect waves-red btn green accent-4 z-depth-3">
  </form>
</div>

