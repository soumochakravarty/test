    <!--Load the AJAX API-->
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
    <script type="text/javascript">

    // Load the Visualization API and the piechart package.
    google.charts.load('current', {'packages':['corechart']});
    
     // Draw the pie chart for Waiter.
     google.charts.setOnLoadCallback(drawDailyAllChart);
     google.charts.setOnLoadCallback(drawDailyIndChart);
     google.charts.setOnLoadCallback(drawWeeklyAllChart);
     google.charts.setOnLoadCallback(drawWeeklyIndChart);
     google.charts.setOnLoadCallback(drawMonthlyAllChart);
     google.charts.setOnLoadCallback(drawMonthlyIndChart);

function drawDailyAllChart() {
  var jsonData = $.ajax({
          url: "/api/dailyall",
          dataType: "json",
          async: false
          }).responseText;
     //document.write(jsonData);
      // Create our data table out of JSON data loaded from server.
      
      var data = new google.visualization.DataTable(jsonData);

      // Instantiate and draw our chart, passing in some options.
      var chart = new google.visualization.LineChart(document.getElementById('daily_all_div'));
      var waiterchart_options = {title:'OverAll Wait Time Analysis:',
        series: {
          0: {targetAxisIndex: 0},
          1: {targetAxisIndex: 1}
        },
        hAxis: {
          title: 'Time',
          logScale: true,
          textStyle: {
            color: '#01579b',
            fontSize: 13,
            bold: true,
            italic: true
          },
          titleTextStyle: {
            color: '#01579b',
            fontSize: 20,
            bold: true,
            italic: true
          }
        },
        vAxis: {
          title: 'Wait Time',
          logScale: false,
          textStyle: {
            color: '#1a237e',
            fontSize: 13,
            italic: true,
            bold: true
          },
          titleTextStyle: {
            color: '#1a237e',
            fontSize: 20,
            italic: true,
            bold: true
          }
        },
        backgroundColor: '#f1f8e9'
        };
      chart.draw(data, waiterchart_options);
      setTimeout(drawDailyAllChart,10000);
}


